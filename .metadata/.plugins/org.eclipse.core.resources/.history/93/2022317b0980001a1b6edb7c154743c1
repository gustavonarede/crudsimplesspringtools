spring.datasource.url=jdbc:mysql://localhost:3306/mydata?useSSL=false&useTimezone=true&serverTimezone=UTC //URL do banco de dados
spring.datasource.username=root // Usuário do banco
spring.datasource.password=  //senha do banco
spring.datasource.initialization-mode=always
spring.datasource.tomcat.test-while-idle=true
spring.datasource.tomcat.validation-query=SELECT 1

spring.jpa.show-sql=true
spring.jpa.hibernate.ddl-auto=create // estrategia de geração das tabelas
spring.jpa.open-in-view=false
spring.jpa.properties.hibernate.dialect=org.hibernate.dialect.MySQL5Dialect
